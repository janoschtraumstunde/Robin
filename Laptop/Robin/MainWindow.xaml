<Window x:Class="Robin.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="MainWindow" Height="350" Width="525" MinWidth="320" MinHeight="240">
	<Grid Style="{StaticResource TitleWindow}">
		<Label Content="Robin Control Panel" Name="label1" VerticalAlignment="Top" Padding="10" FontSize="18" FontWeight="Normal" Style="{StaticResource TitleLabel}"></Label>
		<CheckBox Content="Enabled" Height="16" Margin="0,16,12,0" Name="checkBox1" VerticalAlignment="Top" IsThreeState="False" IsChecked="False" Padding="4,0,0,0" HorizontalAlignment="Right" Width="59">
			
		</CheckBox>
        <Label Content="Label" Margin="0,45,0,0" Name="label2" VerticalAlignment="Top" Style="{StaticResource DarkLabel}" Padding="10,5" FontSize="14" />
    </Grid>
</Window>
